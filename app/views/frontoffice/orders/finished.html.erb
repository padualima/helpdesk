<%= render "title" %>
<div class="row">
  <!-- Area Chart -->
  <div class="col-xl-12 col-lg-12 ">
    <div class="card shadow">
      <!-- Card Header - Dropdown -->
      <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
        <h6 class="m-0 font-weight-bold text-primary">Seus chamados finalizados</h6>
      </div>
      <!-- Card Body -->
      <div class="card-body">
        <table class="table table-bordered dataTable" id="dataTable" width="100%" cellspacing="0" role="grid" aria-describedby="dataTable_info" style="width: 100%;">
          <thead>
            <tr>
              <th rowspan="1" colspan="1">ID</th>
              <th rowspan="1" colspan="1">Assunto</th>
              <th rowspan="1" colspan="1">De</th>
              <th rowspan="1" colspan="1">Sector chamado</th>
              <th rowspan="1" colspan="1">Finalizado por</th>
              <th rowspan="1" colspan="1">Data Finalizada</th>
            </tr>
          </thead>
          <tbody>
            <% @orders_finished.each do |order| %>
            <tr role="row" class="odd">
              <td class="sorting_1"><%= order.id %></td>
              <td><%= order.title %></td>
              <td><%= order.user.name %></td>
              <td><%= order.sector_called.sector.name %></td>
              <td><%= order.user_finished  %></td>
              <td><%= order.date_finished %></td>
            </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
